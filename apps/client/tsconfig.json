{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "experimentalDecorators":true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "plugins": [
    {
      "name": "@vue/typescript-plugin-vue"
    }
  ],
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
